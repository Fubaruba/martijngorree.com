{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./bin/www",
    "lint": "eslint app.es6.js",
    "build:node": "babel app.es6.js -o app.js",
    "build:javascripts": "babel frontend-js/script.es6.js -o public/javascripts/script.js",
    "build:sass": "sassc -mt compressed scss/style.scss public/stylesheets/style.css",
    "auto-start": "nodemon -e js,scss --exec \"npm run lint && npm run build:sass && npm run build:javascripts && npm run build:node && npm start\" --ignore bin/ --ignore node_modules/ --ignore bower_components/ --ignore public/ --ignore app.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "mongodb": "^2.1.7",
    "mongoose": "^4.4.4",
    "morgan": "~1.6.1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.5.1",
    "eslint": "^1.10.3",
    "nodemon": "^1.8.1",
    "watch": "^0.17.1"
  }
}
